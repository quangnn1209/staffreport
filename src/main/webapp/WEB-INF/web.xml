<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<display-name>School</display-name>
	<welcome-file-list>
		<welcome-file>pages/index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<description></description>
		<display-name>UserServlet</display-name>
		<servlet-name>UserServlet</servlet-name>
		<servlet-class>org.school.servlets.UserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserServlet</servlet-name>
		<url-pattern>/UserServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<description></description>
		<display-name>TimetableScheduleServlet</display-name>
		<servlet-name>TimetableScheduleServlet</servlet-name>
		<servlet-class>org.school.servlets.TimetableScheduleServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TimetableScheduleServlet</servlet-name>
		<url-pattern>/TimetableScheduleServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<description></description>
		<display-name>AuthenticationServlet</display-name>
		<servlet-name>AuthenticationServlet</servlet-name>
		<servlet-class>org.school.servlets.AuthenticationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AuthenticationServlet</servlet-name>
		<url-pattern>/AuthenticationServlet</url-pattern>
	</servlet-mapping>
</web-app>